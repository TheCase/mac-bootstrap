export PATH=/usr/local/bin:/opt/local/bin:/opt/local/sbin:~/Documents/bin:$PATH

# colors
export CLICOLOR=1
export LSCOLORS=ExFxBxDxCxegedabagacad

# make it so I don't have to double tab completion on symlinked dirs
bind 'set mark-symlinked-directories on'

export EDITOR=vim

alias rar=unrar
export PATH="$PATH:/usr/local/sbin:~/Development/go/bin"
export GOPATH="~/Development/go"


